<!--
Create tabs with an icon and label, using the tabs-positive style.
Each tab's child <ion-nav-view> directive will have its own
navigation history that also transitions its views in and out.
-->
<ion-tabs class="tabs-icon-top tabs-color-active-positive">

  <!-- Dashboard Tab -->
  <ion-tab title="网格管理" icon-off="ion-ios-bookmarks-outline" icon-on="ion-ios-bookmarks" href="#/tab/dash">
    <ion-nav-view name="tab-dash"></ion-nav-view>
  </ion-tab>

  <!-- management Tab -->
  <ion-tab title="政务管理" icon-off="ion-ios-paper-outline" icon-on="ion-ios-paper" href="#/tab/management">
    <ion-nav-view name="tab-management"></ion-nav-view>
  </ion-tab>
  
  <!-- Chats Tab -->
  <ion-tab title="工作消息" icon-off="ion-ios-people-outline" icon-on="ion-ios-people" href="#/tab/chats">
    <ion-nav-view name="tab-chats"></ion-nav-view>
  </ion-tab>
  
    <!-- management Tab -->
  <ion-tab title="本地要闻" icon-off="ion-ios-information-outline" icon-on="ion-ios-information" href="#/tab/news">
    <ion-nav-view name="tab-news"></ion-nav-view>
  </ion-tab>
  
  <!-- department Tab -->
  <ion-tab title="部门管理"  icon-off="ion-ios-box-outline" icon-on="ion-ios-box" href="#/tab/department">
    <ion-nav-view name="tab-department"></ion-nav-view>
  </ion-tab>
  
  <!-- Account Tab -->
  <ion-tab title="系统设置" hidden="{{hideAccount}}" icon-off="ion-ios-gear-outline" icon-on="ion-ios-gear" href="#/tab/account">
    <ion-nav-view name="tab-account"></ion-nav-view>
  </ion-tab>


</ion-tabs>
