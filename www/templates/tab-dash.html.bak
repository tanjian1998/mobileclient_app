
 <style type="text/css">
 
#container{
 
height: 100%;
 
width: 100%;
 
}
 
section,.AMapArea{
 
height: 100%!important;
 
margin: 0px;
 
padding: 0px;
 
}
 
</style>
 
<ion-view view-title="景区地图">
    <ion-nav-buttons side="left">
      <button class="button  button-icon icon ion-navicon" ng-click="locationAppSetting()">
        <!--应用设置-->
      </button>
    </ion-nav-buttons>
    <ion-nav-buttons side="right">
      <button class="button  button-icon icon ion-settings" ng-click="mapSetting()">
         <!--地图设置-->
      </button>
    </ion-nav-buttons>
<ion-content>
<div id="" class="bar bar-header" ng-show='btnBarShow'>
 
  <div class="button-bar">
  <!--<a class="button button-positive" href="#/tab/dashbuilding">文化遗产</a>-->  
  <button class="button button-positive" ng-click="beginDraw('H')">文化遗产</button>
  <a class="button button-calm" ng-click="beginDraw('I')">特定建筑</a>
  <a class="button button-balanced" ng-click="beginDraw('M')">男员工</a>
  <a class="button button-balanced" ng-click="beginDraw('W')">女员工</a>
  <a class="button button-dark"  ng-click="beginDraw('T')">车辆设备</a>
</div>
</div>
  <!--切换开关-->
 <div class="list" ng-show='btntoggleShow'>
    
    <div class="item item-divider"> 
      Settings
    </div>
    
    <ion-toggle ng-repeat="item in settingsList"
                ng-model="item.checked" 
                ng-checked="item.checked">
      {{ item.text }}
    </ion-toggle>
    
    <div class="item">
    	<!-- 使用 pre 标签展示效果更美观 -->
      <div ng-bind="settingsList | json"></div> 
    </div>
    
    <div class="item item-divider"> 
      Notifications
    </div>
    
    <ion-toggle ng-model="pushNotification.checked"
                ng-change="pushNotificationChange()">
      Push Notifications
    </ion-toggle>
    
    <div class="item">
    	<!-- 使用 pre 标签展示效果更美观 -->
      <div ng-bind="pushNotification | json"></div> 
    </div>
    
    <ion-toggle toggle-class="toggle-assertive"
                ng-model="emailNotification"
                ng-true-value="'Subscribed'"
                ng-false-value="'Unubscribed'">
      Newsletter
    </ion-toggle>
    
    <div class="item">
    	<!-- 使用 pre 标签展示效果更美观 -->
      <div ng-bind="emailNotification | json"></div> 
    </div>
    
  </div>

<div class="AMapArea" id="amap">
 
<div id="{{AMapId}}"></div>
 
{{
initAMap();addFixedIcons();ListenClick();MapRefresh();isInitialed=true;
}}
 
<!---->{{}}
<!---->{{}}
 
</div>
 
</ion-content>
 
</ion-view>
